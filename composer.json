{
    "name": "sindyko/modular-modify",
    "description": "Modularize your Laravel apps",
    "keywords": [
        "laravel",
        "modules",
        "modular",
        "module"
    ],
    "authors": [
        {
            "name": "sindyko",
            "email": "mail@sindyko.ru"
        }
    ],
    "type": "library",
    "license": "MIT",
    "require": {
        "php": "^8.0.2",
        "ext-simplexml": "*",
        "ext-dom": "*",
        "composer/composer": "^2.1",
        "illuminate/support": "^9.0|^10.0|^11.0|^12.0|13.x-dev"
    },
    "require-dev": {
        "orchestra/testbench": "^7.52|^8.33|^9.11|^10.0",
        "friendsofphp/php-cs-fixer": "^3.14",
        "mockery/mockery": "^1.5",
        "phpunit/phpunit": "^9.5|^10.5|^11.5",
        "ext-json": "*",
        "livewire/livewire": "^2.5|^3.0",
        "composer/package-versions-deprecated": "^1.11"
    },
    "autoload": {
        "psr-4": {
            "Sindyko\\ModularModify\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Sindyko\\ModularModify\\Tests\\": "tests/"
        }
    },
    "scripts": {
        "fix-style": "vendor/bin/php-cs-fixer fix",
        "check-style": "vendor/bin/php-cs-fixer fix --diff --dry-run"
    },
    "extra": {
        "laravel": {
            "providers": [
                "Sindyko\\ModularModify\\Support\\ModularServiceProvider",
                "Sindyko\\ModularModify\\Support\\ModularizedCommandsServiceProvider",
                "Sindyko\\ModularModify\\Support\\ModularEventServiceProvider"
            ],
            "aliases": {
                "Modules": "Sindyko\\ModularModify\\Support\\Facades\\Modules"
            }
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
